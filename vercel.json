{
    "version": 2,
    "routes": [
      {
        "src": "/api/index",
        "dest": "/api/index.js"
      }
    ],
    "builds": [
      {
        "src": "nuxt.config.js",
        "use": "@nuxtjs/vercel-builder",
        "config": {
          "serverFiles": ["api/index.js"]
        }
      }
    ]
}